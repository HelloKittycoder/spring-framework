<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="myProcessEngineConfiguration" class="com.kittycoder.processenginefactorybean.engine.MyStandaloneProcessEngineConfiguration">
		<property name="jdbcUrl" value="www.hehe.com"/>
		<property name="jdbcDriver" value="heheDriver"/>
	</bean>

	<bean id="myProcessEngine" class="com.kittycoder.processenginefactorybean.engine.MyProcessEngineFactoryBean">
		<property name="myProcessEngineConfiguration" ref="myProcessEngineConfiguration"/>
	</bean>
</beans>
