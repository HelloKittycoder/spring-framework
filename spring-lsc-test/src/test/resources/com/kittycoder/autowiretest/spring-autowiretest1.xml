<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
	   https://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	   http://www.springframework.org/schema/context
	   https://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- 必须告诉Spring我要使用注解了 https://blog.csdn.net/uotail/article/details/81805827 -->
	<!-- 自动注入：1.未使用注解时（org.springframework.context.frameworktest.ParseConstructorArgTest.test2）
	2.使用注解时（现在就是这种情况） -->
	<context:component-scan base-package="com.kittycoder.autowiretest.po" />

	<bean id="people1" class="com.kittycoder.autowiretest.po.People1"/>

	<!-- 这个bean相当于业务层的service -->
	<bean id="teacher" class="com.kittycoder.autowiretest.po.Teacher"/>
</beans>